<Window x:Class="EpidemicDesign.AddInfo"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EpidemicDesign"
        mc:Ignorable="d"
        Title="Добави нова информация" Height="300" Width="300" Loaded="Window_Loaded">
    <Grid>
        <TextBox x:Name="addInfoTextBox" HorizontalAlignment="Left" Height="155" Margin="27,41,0,0" TextWrapping="Wrap" Text="Добави нова информация в дадената година." VerticalAlignment="Top" Width="241"/>
        <Label x:Name="label" Content="Select Year:" HorizontalAlignment="Left" Margin="43,10,0,0" VerticalAlignment="Top"/>
        <Button x:Name="submitButton" Content="Добави" HorizontalAlignment="Left" Margin="179,214,0,0" VerticalAlignment="Top" Width="75" Click="submitButton_Click"/>
        <Label x:Name="yearLabel" Content="Label" HorizontalAlignment="Left" Margin="118,10,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>